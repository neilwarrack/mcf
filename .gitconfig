[color]
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[alias]
  st = status -sb
  co = checkout
  pr = pull --rebase
  in = "!git remote update -p; git log ..@{u}"
  out = log @{u}..
[core]
  editor = vim
