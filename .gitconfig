[color]
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  frag = magenta
  old = red bold
[alias]
  st = status -sb
  co = checkout
  pr = pull --rebase
  in = "!git remote update -p; git log ..@{u}"
  out = log @{u}..
  # Legit aliases
  switch = !legit switch \"$@\"
  branches = !legit branches
  sprout = !legit sprout \"$@\"
  unpublish = !legit unpublish \"$@\"
  harvest = !legit harvest \"$@\"
  sync = !legit sync \"$@\"
  publish = !legit publish \"$@\"
  graft = !legit graft \"$@\"
[core]
  editor = vim
